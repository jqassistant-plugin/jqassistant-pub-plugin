<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://schema.jqassistant.org/plugin/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant pub Plugin" id="jqa.plugin.pub"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.pub.api.model.DependencyDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.EnvironmentDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.ExecutableDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.FalseSecretDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.FundingDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.GitDependencyDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.HostedDependencyDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.IgnoredAdvisoryDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.PackageDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.PathDependencyDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.PlatformDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.PubDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.ScreenshotDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.SdkDependencyDescriptor</class>
        <class>org.jqassistant.plugin.pub.api.model.TopicDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.pub.impl.PubspecYamlScannerPlugin</class>
    </scanner>
</jqassistant-plugin>
